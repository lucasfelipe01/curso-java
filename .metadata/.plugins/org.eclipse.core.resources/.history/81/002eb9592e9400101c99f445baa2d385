package lambdas;

import java.util.function.Predicate;

public class Predicado {
	
	public static void main(String[] args) {
		
		Predicate<Produto> isCaro = prod -> (prd.preco *(1 - prod.desconto)) >= 750;
		
		Produto produto = new Produto ("Notebook", 3893.89, 015);
		System.out.println();
	}

}
